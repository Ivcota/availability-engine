    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.10",
    "@types/pg": "^8.11.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.21.4",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.4",
